{% for post in tarifsPrestations %}
<div class="col">
    <div class="card">
      <img src="{{ post.data.featuredImage }}" class="card-img-top img-responsive" alt="{{ featuredImage }}"/>
      <div class="card-body">
        <p class="card-text">{{ post.data.description }}</p>
        <h4 class="card-title">{{ post.data.title }}</h4> 
        <h5 class="card-text">A partir de : <br>{{ post.data.prix }}</h5>  
      </div>
    </div>
  </div>
{% endfor %}